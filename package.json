{
    "name": "cattlevuemobile",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "android-serve": "vue-cli-service serve && cordova serve android",
        "ios-serve": "vue-cli-service serve && cordova run ios",
        "android": "vue-cli-service build && cordova run android",
        "ios": "vue-cli-service build && cordova run ios",
        "browser": "vue-cli-service build && cordova run browser",
        "lint": "vue-cli-service lint",
        "full": "vue-cli-service build && cordova run android --release -- --keystore=./keystore.jks --storePassword=password --alias=user --password=password"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0-rc.1",
        "@mdi/font": "^2.8.94",
        "animate.css": "^3.7.0",
        "axios": "^0.18.1",
        "cordova-android": "^7.1.4",
        "cordova-ios": "^5.1.1",
        "cordova-plugin-camera": "^4.0.3",
        "cordova-plugin-file": "^6.0.1",
        "cordova-plugin-splashscreen": "^5.0.3",
        "cordova-plugin-statusbar": "^2.4.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-plugin-wkwebview-engine": "^1.2.1",
        "moment": "^2.22.2",
        "vue": "^2.5.17",
        "vue-loading-overlay": "^3.2.0",
        "vue-moment": "^4.0.0",
        "vue-router": "^3.0.1",
        "vuejs-datepicker": "^1.5.3",
        "vuetify": "^1.2.0",
        "vuex": "^3.0.1",
        "vuex-pathify": "^1.1.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.1",
        "@vue/cli-service": "^3.0.1",
        "cordova-plugin-inappbrowser": "^4.1.0",
        "vue-cli-plugin-vuetify": "^0.2.0",
        "vue-template-compiler": "^2.5.17"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-camera": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-file": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-wkwebview-engine": {},
            "cordova-plugin-inappbrowser": {}
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}